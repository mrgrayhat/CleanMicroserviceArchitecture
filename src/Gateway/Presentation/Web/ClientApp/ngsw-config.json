{
  "index": "/index.html",
  "appData": {
    "name": "Gateway.Web.Api"
  },
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": ["/favicon.ico", "/index.html", "/*.css", "/*.js"]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": ["/assets/**"]
      }
    },
    {
      "name": "fonts",
      "resources": {
        "urls": ["https://fonts.googleapis.com/**", "https://fonts.gstatic.com/**", "/fontawesome-webfont.*"]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api-freshness",
      "urls": ["/api/**", "/connect/authorize"],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 0,
        "maxAge": "0u"
      }
    },
    {
      "name": "api-performance",
      "urls": [],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 100,
        "maxAge": "3d"
      }
    }
  ]
}
